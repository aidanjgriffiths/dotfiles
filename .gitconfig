[includeIf "gitdir:~/github/"]
    path = ~/github/.gitconfig
[includeIf "gitdir:~/dotfiles/"]
      path = ~/github/.gitconfig
[includeIf "gitdir:~/dev/"]
    path = ~/dev/.gitconfig
[core]
        editor = nvim
[sequence]
        editor = nvim
[color]
        branch = auto
        diff = auto
        interactive = auto
        status = auto
        grep = auto
        ui = auto
